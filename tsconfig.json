{
    "compilerOptions": {
        "incremental": true,
        "target": "ES2018",
        "sourceMap": true,
        "outDir": "./dist/",
        "removeComments": true,
        "strict": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "esModuleInterop": true,
        "module": "commonjs"
    }
}
